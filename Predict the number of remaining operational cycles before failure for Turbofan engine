        "import pandas as pd\n",
        "\n",
        "# we load the training and test sets\n",
        "train_df = pd.read_csv('/content/test_FD001.txt', sep=' ', header=None)\n",
        "test_df = pd.read_csv('/content/test_FD002.txt', sep=' ', header=None)\n",
        "rul_df = pd.read_csv('/content/test_FD003.txt', sep=' ', header=None)\n",
        "\n",
        "# The last two columns of the dataframes are all NaN, so we drop them.\n",
        "train_df = train_df.dropna(axis=1)\n",
        "test_df = test_df.dropna(axis=1)\n",
        "rul_df = rul_df.dropna(axis=1)\n",
        "\n",
        "columns = ['rows', 'engine_number', 'time_in_cycles'] + [f'op_setting_{i}' for i in range(1, 4)] + [f'sensor_{i}' for i in range(1, 22)]\n",
        "# Adjust column names to match the number of columns after dropping NaNs\n",
        "columns = columns[:train_df.shape[1]]\n",
        "\n",
        "train_df.columns = columns\n",

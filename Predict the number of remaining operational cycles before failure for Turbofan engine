      "source": [
        "# Calculate Remaining Useful Life (RUL) for the training data\n",
        "# RUL is the number of cycles remaining before failure for each engine.\n",
        "# We assume that the last cycle for each engine in the training data is the point of failure.\n",
        "max_cycles_per_engine = train_df.groupby('engine_number')['time_in_cycles'].max().reset_index()\n",
        "max_cycles_per_engine.columns = ['engine_number', 'max_cycles']\n",
        "\n",
        "train_df = train_df.merge(max_cycles_per_engine, on='engine_number', how='left')\n",
        "print(train_df.columns) # Add this line to check columns after merge\n",
        "train_df['RUL'] = train_df['max_cycles'] - train_df['time_in_cycles']\n",
        "\n",
        "display(train_df.head())"
      ],
